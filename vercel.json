{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "HF_HOME": "/tmp/hf",
    "TRANSFORMERS_CACHE": "/tmp/hf"
  }
}
